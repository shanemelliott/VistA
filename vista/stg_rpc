#default:on
#vistalink connection
#
#
service stg_rpct
{
        type = UNLISTED
        disable = no
        flags = REUSE
        socket_type = stream
        protocol = tcp
        port = 19301
        wait = no
        user = irisowner
        env = TZ=/etc/localtime
        #server = /usr/bin/irissession
        server = /home/irisowner/bin/irissession
        server_args = iris -ci -U VISTA CACHEVMS^XWBTCPM
        instances = UNLIMITED
        per_source = UNLIMITED
}